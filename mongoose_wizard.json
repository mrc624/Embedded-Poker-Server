{
  "version": "1.0.1",
  "level": 0,
  "ui": {
    "production": false,
    "brand": "Brand Name",
    "logo": "<?xml version=\"1.0\" encoding=\"utf-8\"?>\n    <svg viewBox=\"88.087 148.705 306.419 146.216\" xmlns=\"http://www.w3.org/2000/svg\">\n      <rect x=\"88.087\" y=\"148.705\" width=\"306.419\" height=\"146.216\" rx=\"27.838\" ry=\"27.838\" style=\"stroke: rgb(0, 0, 0); stroke-width: 0px; fill: rgba(0, 0, 0, 0.04);\"/>\n      <text style=\"fill: rgba(0, 0, 0, 0.13); font-family: Arial, sans-serif; font-size: 51px; font-weight: 700; white-space: pre;\" x=\"141.873\" y=\"240.891\">My Logo</text>\n    </svg>",
    "toolbar": {"label": "My product console"},
    "buttons": "metal",
    "css": ".brandname { font-weight: bold; color: #677589; }\n.card, .p1 { padding: 0.25em 0.8em; }\n.panel, .p2 { padding: 0.5em 0.8em; }\n.card, .panel, .box { background: white; border: 1px solid #ddd; border-radius: 0.5em; }\n.row { display: flex; }\n.page, .panel, .col { display: flex; flex-direction: column; }\n.size-equal { flex: 1 1 0; }\n.size-auto { flex: 1 1 auto; }\n.title, .small { font-size: 95%; }\n.note { background: #dcfce7; border: 1px solid #86efac; color: #0c4a6e; }\n.note > * { font-size: 90%; }\n.val, .title, .nowrap { white-space: nowrap; text-overflow: ellipsis; }\n.val, .big { font-size: 125%; }\n.card { background: #e2e8f0; border: 1px solid #cbd5e1; }\n.val, .title, .bold { font-weight: bold; }\n.val, .title, .slate { color: #475569; }\n.label { color: #475569; font-size: 95%; }\n.link { color: blue; text-decoration: underline; }\n.page { padding: 0.75em; gap: 0.75em; min-height: 2em; }\n.wrap { flex-wrap: wrap; }\n@media (max-width: 640px) {\n.col, .row { flex-wrap: wrap; }\n.panel { flex: 1 1 100% !important;}\n",
    "api": {
      "reboot": {"type": "action", "read_level": 3, "write_level": 7, "value": false},
      "firmware_update": {"type": "ota", "read_level": 3, "write_level": 7},
      "file_upload": {"type": "upload", "read_level": 3, "write_level": 3},
      "state": {"type": "object", "readonly": true, "attributes": {"speed": {"type": "int", "value": 42}, "temperature": {"type": "int", "value": 27}, "humidity": {"type": "int", "value": 70}, "uptime": {"type": "int", "value": 10}, "version": {"type": "string", "value": "1.0.0", "size": 20}, "online": {"type": "bool", "value": true}, "lights": {"type": "bool", "value": false}, "level": {"type": "int", "value": 83}}},
      "leds": {"type": "object", "read_level": 3, "write_level": 3, "attributes": {"led1": {"type": "bool", "value": false}, "led2": {"type": "bool", "value": true}, "led3": {"type": "bool", "value": false}}},
      "settings": {"type": "object", "read_level": 3, "write_level": 3, "attributes": {"string_val": {"type": "string", "value": "edit & save me", "size": 40}, "log_level": {"type": "int", "value": 2}, "double_val": {"type": "double", "value": 42.42}, "int_val": {"type": "int", "value": 17}, "bool_val": {"type": "bool", "value": true}}},
      "security": {"type": "object", "read_level": 7, "write_level": 7, "attributes": {"admin_password": {"type": "string", "value": "admin", "size": 40}, "user_password": {"type": "string", "value": "user", "size": 40}}},
      "events": {"type": "array", "name": "event", "size": 125, "attributes": {"priority": {"type": "int", "value": 1}, "active": {"type": "bool", "value": true}, "message": {"type": "string", "value": "some event", "size": 50}}}
    },
    "pages": [
      {
        "title": "Dashboard",
        "icon": "desktop",
        "level": 0,
        "classes": "page",
        "layout": [
          {
            "classes": "row gap-3",
            "layout": [
              {"classes": "size-equal card", "layout": [{"format": "Speed", "classes": "title"}, {"format": "${state.speed} km/h", "classes": "val"}]},
              {"classes": "size-equal card", "layout": [{"format": "Temperature", "classes": "title"}, {"format": "${state.temperature} Â°C", "classes": "val"}]},
              {"classes": "size-equal card", "layout": [{"format": "Humidity", "classes": "title"}, {"format": "${state.humidity}%", "classes": "val"}]},
              {"classes": "size-equal card", "layout": [{"format": "Uptime", "classes": "title"}, {"format": "${state.uptime}s", "classes": "val"}]},
              {"classes": "size-equal card", "layout": [{"format": "Firmware version", "classes": "title"}, {"format": "${state.version}", "classes": "val", "css": "color: teal;"}]}
            ]
          },
          {
            "classes": "row gap-3",
            "layout": [
              {
                "classes": "col gap-3",
                "layout": [
                  {
                    "classes": "row gap-3 items-start",
                    "layout": [
                      {
                        "classes": "panel gap-1",
                        "css": "flex: 0 0 33%;",
                        "layout": [
                          {"classes": "title", "format": "LED control panel"},
                          {"classes": "row justify-between items-center", "layout": [{"classes": "label", "format": "LED1"}, {"type": "toggle", "ref": "leds.led1", "autosave": true}]},
                          {"classes": "row justify-between items-center", "layout": [{"classes": "label", "format": "LED2"}, {"type": "toggle", "ref": "leds.led2", "autosave": true}]},
                          {"classes": "row justify-between items-center", "layout": [{"classes": "label", "format": "LED3"}, {"type": "toggle", "ref": "leds.led3", "autosave": true}]}
                        ]
                      },
                      {"classes": "panel note size-auto gap-1", "layout": [{"classes": "title", "format": "Developer note"}, {"format": "To edit this dashboard, click on \"export\" button to download wizard.json configuration file. Edit it, and import back. See <a href=\"https://mongoose.ws/documentation/#mongoose-wizard\" class=\"link\">documentation</a> for the JSON format details."}, {"format": "It is possible to create an arbitrarily complex layouts by nesting elements."}]}
                    ]
                  },
                  {
                    "classes": "row size-auto gap-3",
                    "layout": [
                      {"classes": "panel size-equal", "layout": [{"classes": "title", "format": "Graph"}, {"format": "(TODO: graph goes here)", "css": "color: silver;"}]},
                      {
                        "classes": "panel  size-equal gap-1",
                        "css": "background: transparent;",
                        "layout": [{"classes": "title", "format": "Gauges"}, {"classes": "row", "css": "gap: 1em;", "layout": [{"classes": "col size-equal", "layout": [{"classes": "label", "format": "Room 1 Temperature"}, {"type": "gauge", "ref": "state.temperature"}]}, {"classes": "col size-equal", "layout": [{"classes": "label", "format": "Room 2 Temperature"}, {"type": "gauge", "ref": "state.temperature", "min": 0, "max": 40, "color": "red"}]}]}]
                      }
                    ]
                  }
                ]
              },
              {
                "classes": "panel size-auto",
                "css": "flex: 0 0 25%",
                "layout": [
                  {"classes": "title", "format": "Right sidebar"},
                  {"classes": "row justify-between items-center", "layout": [{"classes": "label", "format": "Online status"}, {"type": "status", "ref": "state.online", "labels": ["offline", "online"]}]},
                  {"classes": "row justify-between items-center", "layout": [{"classes": "label", "format": "Console status"}, {"type": "status", "ref": "state.lights", "labels": ["offline", "online"]}]},
                  {"classes": "label bold", "format": "Water tank level", "css": "margin-top: 2em;"},
                  {"type": "progress", "ref": "state.level", "color": "amber"},
                  {"format": "Custom html - embedded base64 image, a green square: <img style=\"display:inline;\" width=\"15px\" src=\"data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAoAAAAKCAYAAACNMs+9AAAAFUlEQVR42mNk+M9Qz0AEYBxVSF+FAAhKDveksOjmAAAAAElFTkSuQmCC\" />", "classes": "", "css": "margin-top: 2em; "}
                ]
              }
            ]
          }
        ]
      },
      {"classes": "page", "title": "Events", "icon": "bell", "level": 3, "layout": [{"classes": "panel", "layout": [{"classes": "title", "format": "Events"}, {"format": "(TODO: events table goes here)", "css": "color: silver;"}]}]},
      {
        "title": "Device Settings",
        "icon": "settings",
        "level": 3,
        "classes": "page",
        "layout": [
          {
            "classes": "row items-start gap-3 flex-wrap",
            "layout": [
              {
                "classes": "panel gap-1",
                "css": "flex: 0 0 24em;",
                "layout": [
                  {"classes": "title", "format": "Device Settings"},
                  {"classes": "row justify-between items-center", "layout": [{"classes": "label", "format": "String value"}, {"type": "input", "ref": "settings.string_val"}]},
                  {"classes": "row justify-between items-center", "layout": [{"classes": "label", "format": "Double value"}, {"type": "input", "ref": "settings.double_val"}]},
                  {"classes": "row justify-between items-center", "layout": [{"classes": "label", "format": "Integer value"}, {"type": "input", "ref": "settings.int_val"}]},
                  {"classes": "row justify-between items-center", "layout": [{"classes": "label", "format": "Bool value"}, {"type": "toggle", "ref": "settings.bool_val"}]},
                  {"classes": "row justify-between items-center", "layout": [{"classes": "label", "format": "Dropdown (integer)"}, {"type": "dropdown", "ref": "settings.log_level", "options": "disabled,alert,info,debug", "css": "min-width: 8em;"}]},
                  {"classes": "row justify-end", "layout": [{"type": "savebutton", "ref": "settings"}]}
                ]
              },
              {
                "classes": "panel gap-1",
                "css": "flex: 0 0 24em;",
                "layout": [
                  {"classes": "title", "format": "Firmware Update"},
                  {"classes": "row justify-between items-center", "layout": [{"classes": "label", "format": "Current firmware version"}, {"format": "${state.version}"}]},
                  {"classes": "row justify-between items-center", "layout": [{"classes": "label", "format": "Upload new firmware"}, {"type": "ota", "ref": "firmware_update", "title": "select .bin file", "icon": "ellipsis-horizontal", "accept": ".bin"}]},
                  {"classes": "row justify-between items-center", "layout": [{"classes": "label", "format": "Reboot device"}, {"type": "action", "ref": "reboot", "title": "reboot", "icon": "caret-right"}]}
                ]
              },
              {"classes": "panel gap-1", "css": "flex: 0 0 16em;", "layout": [{"classes": "title", "format": "File uploads"}, {"classes": "row justify-between items-center", "layout": [{"classes": "label", "format": "Upload file"}, {"type": "upload", "ref": "file_upload", "title": "choose file", "icon": "ellipsis-horizontal"}]}]}
            ]
          }
        ]
      },
      {
        "classes": "page",
        "title": "Admin Password",
        "icon": "lock",
        "level": 7,
        "layout": [{"classes": "row items-start gap-3", "layout": [{"classes": "panel gap-1", "css": "flex: 0 0 24em;", "layout": [{"classes": "title", "format": "Change admin password"}, {"classes": "row justify-between items-center", "layout": [{"classes": "label", "format": "New password"}, {"type": "input", "ref": "security.admin_password", "password": true}]}, {"classes": "row justify-end", "layout": [{"type": "savebutton", "ref": "security"}]}]}]}]
      }
    ]
  },
  "http": {"http": true, "https": false, "ui": true, "login": true},
  "mqtt": {"enable": false, "url": "mqtt://broker.hivemq.com:1883"},
  "dns": {"type": "default", "url": "udp://8.8.8.8:53", "captive": false},
  "sntp": {"enable": false, "type": 0, "url": "udp://time.google.com:123", "interval": 3600},
  "modbus": {"enable": false, "port": 502},
  "build": {"board": "unix", "ide": "GCC+make", "rtos": "baremetal"}
